<h1>新規投稿</h1>

<%= form_with model: @post, local: true do |f| %>

  <div class="form-group">
  <label>タイトル</label><br>
  <%= f.text_field :title, title: "form-control" %>
  </div>

  <div class="form-group">
  <label>本文</label>
  <%= f.text_area :content, class: "form-control" %>
  </div>

  <%= f.submit "登録", class: "btn btn-primary"%>
<% end %>